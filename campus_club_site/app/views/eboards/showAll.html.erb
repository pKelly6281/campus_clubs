
<div class="row">
  <div class="col-md-6 col-md-offset-3"> 
  	<% if(@eboard.empty?) %>
  		<h1>There is not currently and eboard for this club. Please add new Executive Members <%= link_to "here ", addEboard_path(:id => @club.id)%>.</h1>
  	<% else %>
  		<table class="table table-hover table-condensed">
  			<tr>
  				<th>Position</th>
  				<th>Name</th>
  				<th></th>
  			<tr>
		<%
			@eboard.each do |e|

		%>
		<tr>
			<td><%=ExecPo.find(e.exec_po_id).position%></td>
			<td> <%=User.find(e.user_id).firstname%> <%=User.find(e.user_id).lastname%></td> 
			<td><%= link_to "Edit Position", edit_eboard_path(e), class: "btn btn-primary btn-sm"%></td>
		</tr>
		<%
			end
		%>
	</table>
	<% end %>
  </div>
</div>